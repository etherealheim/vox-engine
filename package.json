{
  "name": "vox-engine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/check-services.js && npx concurrently \"next dev\" \"npm run dev:services\"",
    "dev:services": "npx concurrently \"npm run dev:scraper\" \"npm run dev:db\"",
    "dev:scraper": "node src/scraper-ui.js",
    "dev:db": "drizzle-kit studio",
    "next-dev": "npx concurrently \"next dev\" \"npm run ensure-db-studio\"",
    "ensure-db-studio": "pkill -f \"drizzle-kit studio\" || true && drizzle-kit studio",
    "services": "node scripts/start-services.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "ts-node --project tsconfig.node.json scripts/migrate.ts",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:init": "ts-node --project tsconfig.node.json scripts/init-db.ts",
    "db:reset": "node scripts/reset-database.js",
    "db:clear": "node scripts/clear-sample-data.js",
    "db:seed": "node scripts/seed-initial-data.js",
    "db:seed-votes": "node scripts/seed-votes.js",
    "db:seed-tweets": "node scripts/seed-tweets.js",
    "db:seed-all": "npm run db:seed && npm run db:seed-votes && npm run db:seed-tweets",
    "db:check": "node scripts/check-db-counts.js",
    "db:logs": "node scripts/check-system-logs.js",
    "db:test": "node scripts/test-db-utils.js",
    "fetch-tweets": "ts-node --project tsconfig.node.json scripts/fetch-tweets.ts",
    "scrape-votes": "node src/fetch-votes.js",
    "scrape-ui": "node src/scraper-ui.js",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@vercel/postgres": "^0.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.34.1",
    "express": "^4.18.3",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.479.0",
    "next": "14.2.15",
    "next-themes": "^0.4.6",
    "pg": "^8.11.3",
    "puppeteer": "^23.5.3",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.2",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.15",
    "@types/node": "^20",
    "@types/pg": "^8.11.4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.25.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.19.0"
  }
}
